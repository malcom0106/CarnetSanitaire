@model CarnetSanitaire.Web.UI.Models.ModelViewProduction

@{
    ViewData["Title"] = "Create";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h1>Create</h1>

<h4>Production</h4>
<hr />
<div class="row">
    <div class="col-md-4">
        <form asp-action="Create">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <div class="form-group">
                <label asp-for="Identification" class="control-label"></label>
                <input asp-for="Identification" class="form-control" />
                <span asp-validation-for="Identification" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="NombreBallon" class="control-label"></label>
                <input asp-for="NombreBallon" class="form-control" />
                <span asp-validation-for="NombreBallon" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="TemperatureDepartEcs" class="control-label"></label>
                <input asp-for="TemperatureDepartEcs" class="form-control" />
                <span asp-validation-for="TemperatureDepartEcs" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="TemperatureBouclageEcs" class="control-label"></label>
                <input asp-for="TemperatureBouclageEcs" class="form-control" />
                <span asp-validation-for="TemperatureBouclageEcs" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="TypeReseauId" class="control-label"></label>
                <select asp-for="TypeReseauId" asp-items="ViewBag.TypeReseau" class="form-control"></select>
                <span asp-validation-for="TypeReseauId" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="TypeProductionId" class="control-label"></label>
                <select asp-for="TypeProductionId" asp-items="ViewBag.TypeProduction" class="form-control"></select>
                <span asp-validation-for="TypeProductionId" class="text-danger"></span>
            </div>
            <div class="form-group">
                <input type="submit" value="Create" class="btn btn-primary" />
            </div>
        </form>
    </div>
</div>

<div>
