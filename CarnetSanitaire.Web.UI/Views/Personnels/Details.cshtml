@model CarnetSanitaire.Web.UI.Models.Personnel

@{
    ViewData["Title"] = "Details";
}

<h1>Details</h1>

<div>
    <h4>Personnel</h4>
    <hr />
    <dl class="row col-md-8">
        <dt class = "col-sm-5">
            @Html.DisplayNameFor(model => model.Nom)
        </dt>
        <dd class = "col-sm-7">
            @Html.DisplayFor(model => model.Nom)
        </dd>
        <dt class = "col-sm-5">
            @Html.DisplayNameFor(model => model.Prenom)
        </dt>
        <dd class = "col-sm-7">
            @Html.DisplayFor(model => model.Prenom)
        </dd>
        <dt class = "col-sm-5">
            @Html.DisplayNameFor(model => model.Societe)
        </dt>
        <dd class = "col-sm-7">
            @Html.DisplayFor(model => model.Societe.Nom)
        </dd>
    </dl>
    <div class="row col-md-8">
        <a asp-action="Edit" asp-route-id="@Model.Id" class="btn btn-outline-info col-12 col-md-6">Edit</a>
        <a asp-action="Index" class="btn btn-outline-danger col-12 col-md-6">Retour</a>
    </div>
</div>
