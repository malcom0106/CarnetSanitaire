@model IEnumerable<CarnetSanitaire.Web.UI.Models.Diagnostique>

@{
    ViewData["Title"] = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Diagnostique_Realise)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Diagnostique_Date)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Diagnostique_Intervenant)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Diagnostique_Realise)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Diagnostique_Date)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Diagnostique_Intervenant)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.Id">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.Id">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.Id">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
